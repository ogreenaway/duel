{
  "name": "server",
  "version": "0.0.0",
  "scripts": {
    "build": "ts-node ./scripts/build.ts",
    "clean-install": "rm -rf ./node_modules && rm -r package-lock.json && npm i",
    "dev": "cross-env NODE_ENV=development ts-node ./src",
    "dev:hot": "nodemon --exec \"npm run dev\" --watch ./src --ext .ts",
    "import-data": "ts-node ./src/scripts/import-data.ts",
    "lint": "eslint .",
    "start": "cross-env NODE_ENV=production node -r ./config.js ./dist",
    "test": "jest",
    "type-check": "tsc --noEmit"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "cookie-parser": "^1.4.7",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "helmet": "^8.1.0",
    "inserturlparams": "^2.0.5",
    "jet-env": "^1.1.4",
    "jet-logger": "^2.0.1",
    "jet-paths": "^1.1.0",
    "jet-validators": "^1.4.3",
    "jsonfile": "^6.2.0",
    "module-alias": "^2.2.3",
    "mongodb": "7.0",
    "morgan": "^1.10.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^9.39.1",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@swc/core": "^1.15.3",
    "@types/cookie-parser": "^1.4.10",
    "@types/find": "^0.2.4",
    "@types/fs-extra": "^11.0.4",
    "@types/jsonfile": "^6.1.4",
    "@types/module-alias": "^2.0.4",
    "@types/morgan": "^1.9.10",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.10.1",
    "cross-env": "^10.1.0",
    "jest": "^29.7.0",
    "eslint": "^9.39.1",
    "eslint-plugin-n": "^17.23.1",
    "find": "^0.3.0",
    "fs-extra": "^11.3.2",
    "jiti": "^2.6.1",
    "nodemon": "^3.1.11",
    "prettier": "3.7.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.48.1",
    "vitest": "^4.0.14"
  }
}
